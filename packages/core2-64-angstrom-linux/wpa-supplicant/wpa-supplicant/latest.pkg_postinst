\t# If we're offline, we don't need to do this.\n\tif [ "x$D" = "x" ]; then\n\t\tkillall -q -HUP dbus-daemon || true\n\tfi\n\nOPTS=""\n\nif [ -n "$D" ]; then\n    OPTS="--root=$D"\nfi\n\nif type systemctl >/dev/null 2>/dev/null; then\n\tif [ -z "$D" ]; then\n\t\tsystemctl daemon-reload\n\tfi\n\n\tsystemctl $OPTS disable wpa_supplicant.service wpa_supplicant-nl80211@.service wpa_supplicant-wired@.service\n\n\tif [ -z "$D" -a "disable" = "enable" ]; then\n\t\tsystemctl --no-block restart wpa_supplicant.service wpa_supplicant-nl80211@.service wpa_supplicant-wired@.service\n\tfi\nfi\n