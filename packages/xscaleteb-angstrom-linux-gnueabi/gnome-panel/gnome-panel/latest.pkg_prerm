#!/bin/sh
SCHEMA_LOCATION=/etc/gconf/schemas
for SCHEMA in fish.schemas panel-global.schemas window-list.schemas clock.schemas panel-compatibility.schemas panel-general.schemas panel-object.schemas workspace-switcher.schemas panel-toplevel.schemas; do
	if [ -e $SCHEMA_LOCATION/$SCHEMA ]; then
		HOME=/root GCONF_CONFIG_SOURCE=`gconftool-2 --get-default-source` \
			gconftool-2 \
			--makefile-uninstall-rule $SCHEMA_LOCATION/$SCHEMA > /dev/null
	fi
done
