#!/bin/sh\n# Begin section update-alternatives\nupdate-alternatives --install /usr/bin/gpsd-defaults gpsd-defaults /etc/default/gpsd.default 10\n# End section update-alternatives\nOPTS=""\n\nif [ -n "$D" ]; then\n    OPTS="--root=$D"\nfi\n\nif type systemctl >/dev/null 2>/dev/null; then\n\tif [ -z "$D" ]; then\n\t\tsystemctl daemon-reload\n\tfi\n\n\tsystemctl $OPTS enable gpsd.socket\n\n\tif [ -z "$D" -a "enable" = "enable" ]; then\n\t\tsystemctl --no-block restart gpsd.socket\n\tfi\nfi\n