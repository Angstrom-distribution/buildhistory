#!/bin/sh\n# Begin section update-alternatives\nupdate-alternatives --remove  gpsd-defaults /etc/default/gpsd.default\n# End section update-alternatives\nOPTS=""\n\nif [ -n "$D" ]; then\n    OPTS="--root=$D"\nfi\n\nif type systemctl >/dev/null 2>/dev/null; then\n\tif [ -z "$D" ]; then\n\t\tsystemctl stop gpsd.socket\n\tfi\n\n\tsystemctl $OPTS disable gpsd.socket\nfi\n