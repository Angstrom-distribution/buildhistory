	cd /boot
	for DTB in imx23-olinuxino.dtb
	do
		DTB_BASE_NAME=`basename ${DTB} | awk -F "." '{print $1}'`
		DTB_SYMLINK_NAME=`echo uImage-imx233-olinuxino-nano | sed "s/imx233-olinuxino-nano/${DTB_BASE_NAME}/g"`
		update-alternatives --remove ${DTB_BASE_NAME}.dtb /boot/devicetree-${DTB_SYMLINK_NAME}.dtb 40404 || true
	done
