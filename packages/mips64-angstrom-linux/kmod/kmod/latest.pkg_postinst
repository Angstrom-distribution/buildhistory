#!/bin/sh
	update-alternatives --install /sbin/insmod insmod /sbin/insmod.kmod 60
	update-alternatives --install /sbin/modprobe modprobe /sbin/modprobe.kmod 60
	update-alternatives --install /sbin/rmmod rmmod /sbin/rmmod.kmod 60
	update-alternatives --install /sbin/modinfo modinfo /sbin/modinfo.kmod 60
	update-alternatives --install /bin/lsmod bin-lsmod /bin/lsmod.kmod 60
	update-alternatives --install /sbin/lsmod lsmod /bin/lsmod.kmod 60
	update-alternatives --install /sbin/depmod depmod /sbin/depmod.kmod 60
