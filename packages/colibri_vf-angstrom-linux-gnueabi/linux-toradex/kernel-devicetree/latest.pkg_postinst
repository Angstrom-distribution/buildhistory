	cd /boot
	for DTB in  vf500-colibri-eval-v3.dtb vf610-colibri-eval-v3.dtb
	do
		DTB_BASE_NAME=`basename ${DTB} | awk -F "." '{print $1}'`
		DTB_SYMLINK_NAME=`echo zImage-colibri-vf | sed "s/colibri-vf/${DTB_BASE_NAME}/g"`
