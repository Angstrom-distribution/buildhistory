	cd /boot
	for DTB in imx6sx-sabreauto.dtb imx6sx-sabreauto-m4.dtb
	do
		DTB_BASE_NAME=`basename ${DTB} | awk -F "." '{print $1}'`
		DTB_SYMLINK_NAME=`echo zImage-imx6sxsabreauto | sed "s/imx6sxsabreauto/${DTB_BASE_NAME}/g"`
		update-alternatives --install /boot/${DTB_BASE_NAME}.dtb ${DTB_BASE_NAME}.dtb /boot/devicetree-${DTB_SYMLINK_NAME}.dtb 40101 || true
	done
